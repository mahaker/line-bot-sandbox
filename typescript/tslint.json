{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "max-line-length": false,
        "indent": [true, "spaces", 4],
        "interface-name": false,
        "no-console": false,
        "eofline": true,
        "quotemark": [
            true,
            "single"
        ],
        "curly": false,
        "forin": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
